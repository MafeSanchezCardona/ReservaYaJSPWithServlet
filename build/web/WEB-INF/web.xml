<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>UnidadResidencialServlet</servlet-name>
        <servlet-class>controladores.UnidadResidencialServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarUnidadResidencialServlet</servlet-name>
        <servlet-class>controladores.GuardarUnidadResidencialServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UnidadResidencialServlet</servlet-name>
        <url-pattern>/UnidadResidencialServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarUnidadResidencialServlet</servlet-name>
        <url-pattern>/GuardarUnidadResidencialServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
